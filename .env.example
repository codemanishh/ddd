# TableServe Environment Variables
# Copy this file to .env and fill in your values
# NOTE: For production, use Replit Secrets instead of .env files

# Database (automatically provided by Replit)
DATABASE_URL=
PGHOST=
PGPORT=
PGUSER=
PGPASSWORD=
PGDATABASE=

# Authentication
JWT_SECRET=your-secure-jwt-secret-here
SESSION_SECRET=your-secure-session-secret-here

# Email Service (Maileroo)
MAILEROO_API_KEY=your-maileroo-api-key-here

# Replit Domain (automatically provided)
REPLIT_DEV_DOMAIN=
REPLIT_DOMAINS=
